<div class="col-lg-8 mx-auto p-4 py-md-5">
    <header class="d-flex align-items-center pb-3 mb-5 border-bottom">
        <a href="/" class="d-flex align-items-center text-body-emphasis text-decoration-none">
            <span class="fs-4">Users table</span>
        </a>
    </header>

    <main>
        <form action="/" method="POST" enctype="multipart/form-data">
            <div class="mb-3">
                <label for="formFile" class="form-label">Импорт пользователей из файла(csv, xml)</label>
                <div class="input-group">
                    <input
                        class="form-control"
                        type="file"
                        name="userImportFile"
                        accept=".xml,.csv"
                        id="formFile" aria-describedby="inputGroupFileAddon04"
                        aria-label="Upload">
                    <button class="btn btn-outline-primary">Загрузить</button>
                </div>
            </div>
            {% if error %}
            <p>Ошибка: {{ error }}</p>
            {% endif %}
        </form>
        {% if uploadInfo %}
            <ul>
                <li>Новых пользователей: {{ uploadInfo.insertCount }}</li>
                <li>Обновлено пользователей: {{ uploadInfo.updateCount }}</li>
                <li>Удалено пользователй: {{ uploadInfo.deleteCount }}</li>
                <li>Всего затронуто строк: {{ uploadInfo.totalCount }}</li>
            </ul>

        {% endif %}
        <form action="/" method="POST">
            <div class="row">
                <div class="col-4">
                    <div class="form-group">
                        <label for="orderBy">Поле для сортировки</label>
                        <select name="orderBy" id="orderBy" class="form-control form-select form-select-sm">
                            {% for field in orderFields %}
                                <option value="{{ field }}" {{ orderBy == field ? 'selected' : '' }}>{{ field }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <label for="orderDirection">Направление сортировки</label>
                        <select id="orderDirection" name="orderDirection" class="form-control form-select form-select-sm">
                            <option value="ASC" {{ orderDirection == 'ASC' ? 'selected' : '' }}>По возрастанию</option>
                            <option value="DESC" {{ orderDirection == 'DESC' ? 'selected' : '' }}>По убыванию</option>
                        </select>
                    </div>
                </div>
               <div class="col-4 align-self-end">
                   <button class="btn btn-primary">Отсортировать</button>
               </div>
            </div>

        </form>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Login</th>
                    <th scope="col">Password</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                </tr>
            </thead>
            <tbody>
                {% for user in users %}
                    <tr>
                        <th scope="row">{{ user.id }}</th>
                        <td>{{ user.login }}</td>
                        <td>{{ user.password }}</td>
                        <td>{{ user.name }}</td>
                        <td>{{ user.email }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
        <nav aria-label="Page navigation example">
            <ul class="pagination">
                {% if curPage > 1 %}
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ curPage - 1 }}{{ pageQueryString }}">
                            <span aria-hidden="true">&laquo;</span>
                        </a>
                    </li>
                {% endif %}

                {% if curPage > 3 %}
                    <li class="page-item">
                        <a class="page-link" href="/?page=1{{ pageQueryString }}">1</a>
                    </li>
                    <li class="page-item">
                        <span class="page-link">...</span>
                    </li>
                {% endif %}


                {% if curPage == 3 %}
                    <li class="page-item">
                        <a class="page-link" href="/?page=1{{ pageQueryString }}">1</a>
                    </li>
                {% endif %}

                {% if curPage > 1 %}
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ curPage - 1 }}{{ pageQueryString }}">{{ curPage - 1 }}</a>
                    </li>
                {% endif %}
                <li class="page-item active">
                    <span class="page-link">{{ curPage }}</span>
                </li>

                {% if curPage < pageCount %}
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ curPage + 1 }}{{ pageQueryString }}">{{ curPage + 1 }}</a>
                    </li>
                {% endif %}

                {% if curPage < pageCount - 2 %}
                    <li class="page-item">
                        <span class="page-link">...</span>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ pageCount }}{{ pageQueryString }}">{{ pageCount }}</a>
                    </li>
                {% endif %}

                {% if curPage == pageCount - 2 %}
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ pageCount }}{{ pageQueryString }}">{{ pageCount }}</a>
                    </li>
                {% endif %}

                {% if curPage < pageCount %}
                    <li class="page-item">
                        <a class="page-link" href="/?page={{ curPage + 1 }}{{ pageQueryString }}">
                            <span aria-hidden="true">&raquo;</span>
                        </a>
                    </li>
                {% endif %}
            </ul>
        </nav>
    </main>
</div>
